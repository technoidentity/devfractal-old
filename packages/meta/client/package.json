{
  "name": "meta-client",
  "version": "1.3.0",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "lib": "tsdx watch --tsconfig tsconfig.build.json",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p tsconfig.json",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --noEmit -p tsconfig.json --watch",
    "sg": "styleguidist server",
    "sg:build": "styleguidist build",
    "sb": "start-storybook",
    "validate": "yarn lint:fix && yarn types && yarn test",
    "validate:noFix": "yarn lint && yarn types && yarn test"
  },
  "dependencies": {
    "date-fns": "2.9.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "chance": "1.1.4",
    "cross-env": "7.0.0",
    "date-fns": "2.9.0",
    "eslint": "6.8.0",
    "formik": "2.1.4",
    "jest": "25.1.0",
    "meta-core": "^1.3.0",
    "parcel-bundler": "1.12.4",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "technoidentity-core": "^1.3.0",
    "technoidentity-crud": "^1.3.0",
    "technoidentity-utils": "^1.3.0",
    "tsdx": "0.12.3",
    "tslint": "6.0.0",
    "typescript": "3.7.5",
    "yup": "0.28.1"
  },
  "peerDependencies": {
    "meta-core": "1.2.5",
    "technoidentity-crud": "1.2.19"
  }
}
