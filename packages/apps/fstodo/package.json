{
  "name": "app-fstodo",
  "version": "1.2.19",
  "private": true,
  "dependencies": {
    "firebase": "7.8.0",
    "formik": "2.1.3",
    "react": "16.12.0",
    "react-datepicker": "2.11.0",
    "react-dom": "16.12.0",
    "@stp/devfractal": "1.2.19",
    "@stp/utils": "1.2.19",
    "yup": "0.28.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@types/chance": "1.0.8",
    "@types/jest": "25.1.1",
    "@types/node": "13.7.0",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/yup": "0.26.29",
    "babel-eslint": "10.0.3",
    "chance": "1.1.4",
    "confusing-browser-globals": "1.0.9",
    "cross-env": "7.0.0",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "json-server": "0.15.1",
    "parcel-bundler": "1.12.4",
    "prettier": "1.19.1",
    "react-styleguidist": "10.6.1",
    "react-testing-library": "8.0.1",
    "rimraf": "3.0.1",
    "ts-jest": "25.2.0",
    "tslint": "6.0.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.2.0",
    "typescript": "3.7.5"
  },
  "scripts": {
    "start": "parcel index.html --open",
    "server": "json-server data/todos.json",
    "build": "parcel build index.html",
    "clean": "rimraf dist .cache",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "tlint": "yarn tslint -p . 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p .",
    "types:watch": "yarn types --watch",
    "validate": "yarn lint:fix && yarn types && yarn test",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "sg": "styleguidist server",
    "sg:build": "styleguidist build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
