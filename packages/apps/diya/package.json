{
  "name": "apps-diya-client",
  "version": "1.2.20",
  "private": true,
  "scripts": {
    "start": "parcel index.html --open",
    "server": "cross-env PORT=9999 ts-node --project tsconfig.tsnode.json src/db.ts",
    "build": "parcel build index.html",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p .",
    "types:watch:verbose": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch --preserveWatchOutput",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "dependencies": {
    "@react-google-maps/api": "1.8.3",
    "@types/googlemaps": "3.39.2",
    "date-fns": "2.9.0",
    "formik": "2.1.4",
    "react": "16.12.0",
    "react-autosuggest": "9.4.3",
    "react-dom": "16.12.0",
    "technoidentity-devfractal": "^1.2.20",
    "technoidentity-utils": "^1.2.20",
    "yup": "0.28.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "@types/jest": "25.1.2",
    "@types/node": "13.7.0",
    "@types/react": "16.9.19",
    "@types/react-autosuggest": "9.3.13",
    "@types/yup": "0.26.30",
    "bulmaswatch": "0.7.5",
    "cross-env": "7.0.0",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "node-sass": "4.13.1",
    "parcel-bundler": "1.12.4",
    "prettier": "1.19.1",
    "react-datepicker": "2.12.0",
    "technoidentity-dev-utils": "^1.2.20",
    "ts-jest": "25.2.0",
    "ts-node": "8.6.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.2.0",
    "typescript": "3.7.5",
    "typescript-tslint-plugin": "0.5.5"
  },
  "proxy": "http://localhost:9999",
  "resolutions": {
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
