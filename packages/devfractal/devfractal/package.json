{
  "name": "technoidentity-devfractal",
  "version": "1.2.18",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/technoidentity-devfractal.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "precommit": "yarn lint-staged",
    "prepush": "yarn validate:noFix",
    "start": "parcel index.html --open",
    "lib": "tsdx watch",
    "build": "tsdx build",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p .",
    "types:watch:verbose": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch --preserveWatchOutput",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "dependencies": {
    "devfractal-api": "^1.2.18",
    "devfractal-crud": "^1.2.18",
    "devfractal-forms": "^1.2.18",
    "devfractal-router": "^1.2.18",
    "devfractal-simple": "^1.2.18",
    "devfractal-ui": "^1.2.18",
    "devfractal-ui-api": "^1.2.18",
    "devfractal-ui-core": "^1.2.18",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.3",
    "@types/chance": "1.0.6",
    "@types/cleave.js": "1.4.0",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.5",
    "@types/react-test-renderer": "16.9.0",
    "@types/yup": "0.26.23",
    "babel-eslint": "10.0.3",
    "eslint": "6.2.2",
    "firebase": "6.4.2",
    "formik": "1.5.8",
    "fp-ts": "2.0.5",
    "husky": "3.0.4",
    "io-ts": "2.0.1",
    "io-ts-types": "0.5.0",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "monocle-ts": "2.0.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-test-render": "1.1.2",
    "react-test-renderer": "16.9.0",
    "rimraf": "3.0.0",
    "ts-jest": "24.0.2",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  },
  "peerDependencies": {
    "firebase": "6.4.2",
    "formik": "1.5.8",
    "fp-ts": "2.0.5",
    "io-ts": "2.0.1",
    "io-ts-types": "2.0.0",
    "monocle-ts": "2.0.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "yup": "0.27.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  }
}
